{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "clean-metro-cache": "rm -rf $TMPDIR/metro-cache",
    "build-local-ios": "run eas build --profile simulator-dev-local -p ios --local",
    "build-local-android": "run eas build --profile simulator-dev-local -p android --local",
    "lint": "run -T eslint ./src",
    "eas-build-post-install": "yarn workspace fk-shid-core compile"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@types/react": "~19.1.10",
    "expo": "^54.0.32",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.22",
    "expo-share-intent": "^5.1.1",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "fk-shid-core": "workspace:^",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "react-native-worklets": "0.5.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
