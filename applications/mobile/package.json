{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "clean-metro-cache": "rm -rf $TMPDIR/metro-cache",
    "build-local-ios": "run eas build --profile simulator-dev-local -p ios --local",
    "build-local-android": "run eas build --profile simulator-dev-local -p android --local",
    "lint": "run -T eslint ./src"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@types/react": "~18.3.12",
    "expo": "^52.0.37",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-share-intent": "^3.0.0",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.1",
    "fk-shid-core": "workspace:^",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "^2.2.1"
