{
  "cli": {
    "version": ">= 7.6.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "simulator-dev-local": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "simulator": true
      },
      "env": {
        "NODE_ENV": "development",
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    },

    "internal-distribution": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "simulator": false
      }
    },
    "latest-dev-client": {},
    "production": {
      "distribution": "internal",
      "developmentClient": true,
      "channel": "latest",
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
