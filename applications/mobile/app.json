{
  "expo": {
    "newArchEnabled": true,
    "name": "Link Laundry",
    "slug": "link-laundry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/link-laundry-dark.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linklaundry.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.linklaundry.mobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-share-intent",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./src/assets/link-laundry-dark.png",
          "imageWidth": 175
        }
      ]
    ],
    "scheme": "link-laundry",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "13029648-6d6f-4050-835c-f5d06324bbf1"
      }
    },
    "owner": "slong.exe",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/13029648-6d6f-4050-835c-f5d06324bbf1"
    }
  }
}
