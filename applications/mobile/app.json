{
  "expo": {
    "newArchEnabled": true,
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fkshid.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fkshid.mobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-share-intent", "expo-router"],

    "scheme": "fk-shid",
    "extra": {
      "eas": {
        "projectId": "5b39c4b9-82a1-4dc7-925a-ee41cef12afc"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "slong.exe",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5b39c4b9-82a1-4dc7-925a-ee41cef12afc"
    }
  }
}
